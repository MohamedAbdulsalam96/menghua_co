 "css": "@font-face {\n    font-family: 'TH Sarabun New';\n    src: url('/assets/menghua_co/fonts/th-sarabun-new/THSarabunNew.ttf') format('truetype');\n}\n\nbody, p, h4, h1, table, ul, ol, li, .footer, .inline-header {\n    font-family: 'TH Sarabun New', sans-serif!important; \n    font-size: 16px;\n    font-weight: normal;\n}\n\n.print-format {\n    padding: 0px;\n    margin-right: 5mm;\n    margin-left: 5mm;\n    margin-top: 0mm;\n    margin-bottom: 0mm;\n}\n\ntable {\n    width: 100%;\n    border-collapse: collapse;\n}\n\nth, td {\n    padding: 0px;\n    text-align: left;\n}\n\nth {\n    color: #000000 !important;\n}\n\n.logo-container {\n    text-align: left; \n    padding: 0;\n    width: 100px;\n    height: 100px;\n    object-fit: contain;\n}\n\n.right-align {\n    text-align: right;\n}\n\n.left-align {\n    text-align: left;\n}\n\n.center-align {\n    text-align: center;\n}\n\n.compact {\n    margin: 0;\n    padding: 0;\n}\n\n.col-1, .col-2, .col-3 {\n    margin: 0;\n}\n\n.custom-table {\n    width: 30%;\n    border-collapse: collapse;\n    margin-top: -10px;\n    margin-bottom: -10px;\n}\n\n.custom-row {\n    display: table-row;\n}\n\n.custom-col-1, .custom-col-2 {\n    display: table-cell;\n    padding: 0px;\n    line-height: 0;\n}\n\n.custom-col-1 {\n    margin: -5;\n    width: 15%;\n    text-align: left;\n    color: #ff6600;\n    text-indent: 50px;\n}\n\n.custom-col-2 {\n    width: 850%;\n    text-align: left;\n    text-indent: 0px;\n}\n\n.border-right {\n    border-right: 0.25px solid white;\n}\n\n.border-bottom {\n    border-bottom: 0.25px solid white;\n}\n\n.with-border {\n    border-bottom: 2px solid #d3d3d3;\n    padding: 0;\n}\n\n.compact-table {\n    width: 100%;\n    border-collapse: collapse;\n    font-size: 12px;\n    line-height: 1;\n}\n.compact-table {\n    width: 100%;\n    border-collapse: collapse;\n    font-size: 11px;\n    line-height: 1;\n}\n.compact-table th, .compact-table td {\n    text-align: center;\n    line-height: 1;\n}\n.compact-table th[colspan=\"5\"], .compact-table th[colspan=\"4\"] {\n    max-width: 200px; \n}\n",
